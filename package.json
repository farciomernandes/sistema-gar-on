  "scripts": {
    "prebuild": "rimraf dist",
    "build": "node_modules/.bin/nest build -p ts-config-build.json",
    "build:digitalocean": "node --max_old_space_size=8192 ./node_modules/.bin/nest build -p ts-config-build.json",
    "start": "NODE_OPTIONS=\"--max_old_space_size=8192\"  node dist/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "seed": "nest build && ts-node dist/infra/db/typeorm/seeds/seed.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typeorm": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli -d dist/infra/db/database.provider.js",
    "migration:create": "npx typeorm migration:create",
    "migration:generate": "npx typeorm migration:generate -d src/infra/db/database.provider.ts",
    "migration:latest": "nest build && npx typeorm migration:run -d dist/infra/db/database.provider.js",
    "migration:revert": "nest build && npx typeorm migration:revert -d dist/infra/db/database.provider.js"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.1",
    "@nestjs/config": "^3.2.3",
