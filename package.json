    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "express-basic-auth": "^1.2.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
